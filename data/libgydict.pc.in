prefix=@prefix@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${exec_prefix}/include

Name: LibGydict
Description: LibGydict consists of the components used to build the Gydict
Version: @VERSION@
Libs: -L${libdir}/gydict -lgydict -legg-private -lpanel-gtk
Cflags: -I{includedir}/gydict/libgydict -I{includedir}/gydict/egg -I{includedir}/gydict/pnl
