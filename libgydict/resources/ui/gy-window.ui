<?xml version="1.0" encoding="UTF-8"?>
<interface>

	<object class="GyTextBuffer" id="text_buffer">
	</object>

	<template class="GyWindow" parent="GtkApplicationWindow">
		<property name="can_focus">False</property>
		<property name="has_focus">False</property>
		<property name="is_focus">False</property>
	  <child type="titlebar">
	    <object class="GyHeaderBar" id="header_bar"></object>
	  </child>
	  <child>
			<object class="GtkBox" id="child_box">
				<property name="visible">True</property>
				<property name="can_focus">False</property>
				<property name="orientation">vertical</property>
				<child>
					<object class="GtkPaned" id="paned">
						<property name="visible">True</property>
						<property name="can_focus">False</property>
	      		<property name="position">200</property>
						<child>
							<object class="GtkBox" id="left_box">
								<property name="visible">True</property>
								<property name="can_focus">False</property>
								<property name="orientation">vertical</property>
								<child>
									<object class="GtkRevealer" id="revealer_buttons">
										<property name="visible">True</property>
										<property name="can_focus">False</property>
										<property name="vexpand">False</property>
										<property name="hexpand">True</property>
										<child>
											<object class="GtkToolbar" id="toolbar_buttons">
												<property name="visible">True</property>
												<property name="can_focus">False</property>
												<property name="show_arrow">False</property>
												<child>
													<object class="GtkToolButton" id="a_button">
														<property name="visible">True</property>
														<property name="can_focus">False</property>
														<property name="is_important">True</property>
														<property name="label" translatable="no">ä</property>
														<property name="use_underline">True</property>
														<property name="action_name">win.paste-sign</property>
														<property name="action_target">"ä"</property>
													</object>
													<packing>
														<property name="expand">True</property>
														<property name="homogeneous">True</property>
													</packing>
												</child>
												<child>
													<object class="GtkToolButton" id="b_button">
														<property name="visible">True</property>
														<property name="can_focus">False</property>
														<property name="is_important">True</property>
														<property name="label" translatable="no">ö</property>
														<property name="use_underline">True</property>
														<property name="action_name">win.paste-sign</property>
														<property name="action_target">"ö"</property>
													</object>
													<packing>
														<property name="expand">True</property>
														<property name="homogeneous">True</property>
													</packing>
												</child>
			  								<child>
			    								<object class="GtkToolButton" id="c_button">
			      								<property name="visible">True</property>
			      								<property name="can_focus">False</property>
			      								<property name="is_important">True</property>
  			      							<property name="label" translatable="no">ü</property>
			      								<property name="use_underline">True</property>
			      								<property name="action_name">win.paste-sign</property>
			      								<property name="action_target">"ü"</property>
			    								</object>
			    								<packing>
			      								<property name="expand">True</property>
  			      							<property name="homogeneous">True</property>
		      	    					</packing>
			  								</child>
			  								<child>
			    								<object class="GtkToolButton" id="d_button">
			      								<property name="visible">True</property>
			      								<property name="can_focus">False</property>
			      								<property name="is_important">True</property>
			      								<property name="label" translatable="no">ß</property>
			      								<property name="use_underline">True</property>
			      								<property name="action_name">win.paste-sign</property>
    			      						<property name="action_target">"ß"</property>
			    								</object>
			    								<packing>
			      								<property name="expand">True</property>
			      								<property name="homogeneous">True</property>
			    								</packing>
			  								</child>
											</object>
  		      				</child>
		    					</object>
		    					<packing>
		      					<property name="expand">False</property>
		      					<property name="fill">False</property>
		      					<property name="position">1</property>
		    					</packing>
		  					</child><!-- end toolbar_buttons-->
    		  			<child>
      		    		<object class="GtkScrolledWindow" id="scrolledwindowtree">
		      					<property name="visible">True</property>
	  	      				<property name="can_focus">True</property>
	    	      			<property name="shadow_type">in</property>
		      					<child>
											<object class="GyTreeView" id="tree_view">
												<property name="visible">True</property>
			  								<property name="can_focus">True</property>
			  								<property name="rules_hint">True</property>
			  								<property name="fixed_height_mode">True</property>
			  								<property name="headers_visible">False</property>
			  								<child internal-child="selection">
			  									<object class="GtkTreeSelection" id="treeview-selection1"/>
			  								</child>
			  								<child>
			  									<object class="GtkTreeViewColumn" id="dict_text_column">
			  										<property name="title">Words</property>
			  										<property name="sizing">fixed</property>
			  										<child>
			  											<object class="GtkCellRendererText" id="dict_text_renderer">
			  											</object>
			  											<attributes>
			  												<attribute name="text">0</attribute>
			  											</attributes>
			  										</child>
			  									</object> <!-- dict_text_column -->
			  								</child>
											</object> <!-- tree_view -->
		 								</child>
		    					</object>
		    					<packing>
		      					<property name="expand">True</property>
		      					<property name="fill">True</property>
		      					<property name="position">2</property>
		    					</packing>
		  					</child>
							</object>
							<packing>
		  					<property name="resize">False</property>
		  					<property name="shrink">False</property>
							</packing>
	      		</child>
	      		<child> <!-- text_box -->
							<object class="GtkBox" id="text_box">
  		  				<property name="visible">True</property>
		  					<property name="can_focus">False</property>
		  					<property name="orientation">vertical</property>
		  					<child> <!--scrolledwindowtext -->
		    					<object class="GtkScrolledWindow" id="scrolledwindowtext">
		      					<property name="visible">True</property>
		      					<property name="can_focus">True</property>
		      					<property name="shadow_type">in</property>
		      					<child> <!-- text_view -->
											<object class="GyTextView" id="text_view">
  			  							<property name="visible">True</property>
  			  							<property name="can_focus">True</property>
  			  							<property name="editable">False</property>
  			  							<property name="margin_top">8</property>
  			 								<property name="margin_bottom">8</property>
  			  							<property name="pixels_above_lines">4</property>
  			  							<property name="pixels_below_lines">4</property>
  			  							<property name="pixels_inside_wrap">2</property>
  			  							<property name="left_margin">10</property>
  			  							<property name="right_margin">10</property>
  			  							<property name="buffer">text_buffer</property>
											  <property name="wrap-mode">GTK_WRAP_WORD</property>
  										</object>
		      					</child> <!-- end text_view -->
		    					</object>
		    					<packing>
		      					<property name="expand">True</property>
		      					<property name="fill">True</property>
		      					<property name="position">0</property>
		    					</packing>
		  					</child> <!-- end scrolledwindowtext -->
							</object>
							<packing>
		  					<property name="resize">True</property>
		  					<property name="shrink">True</property>
  						</packing>
    	      </child> <!-- end text_box -->
					</object>
					<packing>
						<property name="expand">True</property>
      	    <property name="fill">True</property>
      	    <property name="position">1</property>
      	  </packing>
      	</child>
      </object>
    </child>
  </template>
</interface>
